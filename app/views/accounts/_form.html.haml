= form_for @account do |f|
  = f.error_messages
  %p
    = f.label :display_name
    %br
    = f.text_field :display_name
  %p
    = f.label :eamil
    %br
    = f.text_field :eamil
  %p
    = f.label :phone
    %br
    = f.number_field :phone
  %p
    = f.label :address
    %br
    = f.text_area :address
  %p
    = f.label :refence
    %br
    = f.text_field :refence
  %p
    = f.submit
